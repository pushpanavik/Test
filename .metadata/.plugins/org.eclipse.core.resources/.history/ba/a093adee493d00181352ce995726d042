package com.bridgeit.objectOriented;

import java.util.ArrayList;

interface  AddressBookInterface {

	public String addPerson();
	public String editPersonDetail();
	public String deletePerson(String firstName,String lastName,ArrayList<Person> addentries);
	public int sortBylastName(ArrayList<Person> personlist);
	public int sortByZip(ArrayList<Person> addentries);
	public void printAllEntry(ArrayList<Person> addentries);
	public void printEntry(Person person);
	
	
}